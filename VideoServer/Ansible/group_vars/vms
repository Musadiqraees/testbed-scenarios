collector:
  data_output_port: 5010
  api_port: 7777
  interval: 500ms
  data: /bitflow/data-collector
  data_mounted: /opt/bitflow/data-collector
  hostname_mounted: /etc_host/hostname
  service_name: service
  service_regex: service
  extra_command_params: -proc vnf="{{ service_regex }}|vnf_" -include vnf/ -tag host="{{ service_name }}@$$(cat {{ collector_hostname_mounted }}).ims4"

injector:
